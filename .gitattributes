# SCM syntax highlighting & preventing 3-way merges
pixi.lock merge=binary linguist-language=YAML linguist-generated=true
data/scifi_fantasy.json filter=lfs diff=lfs merge=lfs -text
data/scifi_fantasy_reviews.json filter=lfs diff=lfs merge=lfs -text
data/scifi_fantasy.json.gz filter=lfs diff=lfs merge=lfs -text
data/scifi_fantasy_reviews.json.gz filter=lfs diff=lfs merge=lfs -text
data/*.json.gz filter=lfs diff=lfs merge=lfs -text
